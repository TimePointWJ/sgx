            <script>
                var address_app = function () {

                    var pageSize = 5;
                    var page = 1;
                    var initFlag = true;
                    var ractive;
                    function renderView() {
                        ractive = new Ractive({
                            el: "#electionContainer",
                            template: "#electionTemplate",
                            onrender: function () { },
                            oncomplete: function () {

                                getDataList(1, null)

                            }
                        })
                    }
                    $("#btnSearch111").click(function () {
                        initFlag = true;
                        getDataList(1, null)
                    })
                    var getDataList = function (currPage, jg) {
                        var param = { pageSize: pageSize, page: currPage }
                        param.keyword = $("#keyword").val();
                        param.start = $("#startTime").val();
                        param.end = $("#endTime").val();
                        $.ajax({
                            url: Constants.path + "union/getUionList",
                            type: "post",
                            dataType: 'json',
                            data: JSON.stringify(param),
                            contentType: "application/json",
                            success: function (response) {
                                console.log(response)
                                if (response) {
                                    $("#Pagination").pagination(
                                        {
                                            pageCount: Math.ceil(response.total / pageSize),
                                            totalData: response.total,
                                            showData: pageSize,
                                            callback: function (api) {
                                                getDataList(api.getCurrent(), null)
                                            }//回调函数
                                        });
                                    initFlag = false;

                                    // if (response.data != null && response.data != "") {
                                    //     ractive.set("data", response.data);
                                    // } else {
                                    //     ractive.set("data", "");
                                    // }     
                                    if (response.data != null && response.data != "") {
                                        ractive.set("data", response.data);
                                    } else {
                                        ractive.set("data", "");
                                    }
                                } else {
                                    //暂无数据
                                }
                            }
                        });
                    }
                    return {
                        init: function () {
                            renderView();
                        }
                    }
                }();
                $(document).ready(address_app.init)
            </script>
