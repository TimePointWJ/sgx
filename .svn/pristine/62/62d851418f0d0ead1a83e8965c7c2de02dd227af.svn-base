                        <div class="m-style" style="display: block;float: right">
                            <nav aria-label="Page navigation">
                                <div id="Pagination" class="pagination">

                                </div>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script type="text/ractive" id="lawTemplate">
        {{#if data}}
        {{#each data.data : i}}
        <tr class="no-records-found">
            <td class="bs-checkbox " style="text-align: center; vertical-align: middle; width: 36px;" data-field="state" tabindex="0">
                <div class="th-inner ">
                    <!--<input name="btSelectAll" type="checkbox">-->
                    <section title=".squaredFour">
                        <!-- .squaredFour -->
                        <div class="squaredFour">
                            <input type="checkbox" value="None" id="{{(data.currPage-1)*5+i+2}}" name="squaredFour" onclick="count(this)" />
                            <label for="{{(data.currPage-1)*5+i+2}}"></label>
                        </div>
                        <!-- end .squaredFour -->
                    </section>
                </div>
                <div class="fht-cell"></div>
            </td>
            <td style="text-align: center;" data-field="name" tabindex="0">
                <div class="th-inner sortable">{{(data.currPage-1)*5+i+1}}</div>
                <div class="fht-cell"></div>
            </td>
            <td style="text-align: center;" data-field="name" tabindex="0">
                <div class="th-inner sortable ">{{data.data[i].userName}}</div>
                <div class="fht-cell"></div>
            </td>
            <td style="text-align: center;" data-field="name" tabindex="0">
                <div class="th-inner sortable ">{{data.data[i].higherUnion}}</div>
                <div class="fht-cell"></div>
            </td>
            <td style="text-align: center;" data-field="name" tabindex="0">
                <div class="th-inner sortable ">{{data.data[i].union}}</div>
                <div class="fht-cell"></div>
            </td>
            <td style="text-align: center;" data-field="name" tabindex="0">
                    <div class="th-inner sortable ">{{data.data[i].job}}</div>
                    <div class="fht-cell"></div>
            </td>
            <td style="text-align: center;" data-field="name" tabindex="0">
                {{#if data.data[i].access == '1'}}
                    <div class="th-inner sortable ">工会成员</div>
                {{/if}}
                {{#if data.data[i].access == '2'}}
                    <div class="th-inner sortable ">办公室</div>
                {{/if}}
                {{#if data.data[i].access == '3'}}
                    <div class="th-inner sortable ">分管领导</div>
                {{/if}}
                {{#if data.data[i].access == '4'}}
                    <div class="th-inner sortable ">主要领导</div>
                {{/if}}
                    <div class="fht-cell"></div>
            </td>
            <td style="text-align: center;" data-field="name" tabindex="0">
                    <div class="th-inner sortable ">{{data.data[i].loginId}}</div>
                    <div class="fht-cell"></div>
            </td>
            <td style="text-align: center;" data-field="name" tabindex="0">
                    <div class="th-inner sortable ">{{data.data[i].remarks}}</div>
                    <div class="fht-cell"></div>
            </td>
            <td style="text-align: center;" data-field="name" tabindex="0">
                    <a href='#' on-click="modify" id="modify" data-toggle="modal" data-target="#myModal1"><div class="icon-edit" style="color: red;"></div> 修改</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
                    <a href="#" on-click="sc"><div class="icon-trash" style="color: red;"></div> 删除</a>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
             </td>
        </tr>
        {{/each}}
        {{/if}}
</script>
    <script>
        var address_app = function () {

            var pageSize = 5;

            var initFlag = true;
            var ractive;
            function renderView() {
                ractive = new Ractive({
                    el: "#lawContainer",
                    template: "#lawTemplate",
                    onrender: function () { },
                    oncomplete: function () {
                        getDataList(1, null)
                    }
                })
                ractive.on({
                    modify: function (obj) {
                        var row = obj.context;
                        $("#name1").val(row.userName);
                        $("#account1").val(row.loginId);
                        $("#firm1").val(row.union);
                        $("#position1").val(row.access);
                        $("#role1").val(row.job);
                        //$("#position1").val(row.job);
                        //$("#role1").val(row.identity);
                        $("#telephone1").val(row.loginId);
                        $("#remark1").val(row.remarks);
                        return;
                        //$("#superName1").val(row.superiorUnionName);
                        var id = row.id;
                        store.set("id", id);
                    },
                    sc: function (e) {
                        $.ajax({
                            url: Constants.path + "user/deleteUser/" + e.context.id,
                            type: "get",
                            success: function (response) {
                                alert("删除成功")
                                window.location.href = 'PersonManage.html';
                            }
                        });
                    }
                })
            }
            $("#btnSearch").click(function () {
                initFlag = true;
                getDataList(1, null)
            });
            $("#create").click(function () {
                store.set("createOrUpdate", "create");
            });
            $("#refresh").click(function () {
                window.location.href = 'PersonManage.html';
            });
            // $("#export").click(function () { 
            //     //var param = { pageSize: "5", page: "1" } 
            //     $.ajax({
            //         type: "post",
            //         url: Constants.path + "user/export",
            //         success: function (data) {
            //             alert("导出成功！")
            //         }
            //     })
            //     window.location.href = 'PersonManage.html';
            // });
            $("#export").click(function () { 
                var url = Constants.path + "user/export"; 
                window.open(url); 
            });
            $("#sure").click(function () {
                var param = {};
                param.userName = $("#name").val();
                param.loginId = $("#account").val()
                param.union = $("#firm").val()
                //param.job = $("#position").val()
                param.job = $("#role").val()
                param.remarks = $("#remark").val()
                param.access = $("#position").val()
                $.ajax({
                    type: "post",
                    url: Constants.path + "user/addUser",
                    data: JSON.stringify(param),
                    dataType: "json",
                    contentType: "application/json",
                    success: function (data) {
                        $("#name").val("");
                        $("#superName").val("");
                        alert("新增成功！")
                        $('#myModal').modal('hide');
                        getDataList(1, null)
                    }
                })
            })
            $("#sure1").click(function () {
                var param = {};
                param.userName = $("#name1").val();
                param.loginId = $("#account1").val()
                param.union = $("#firm1").val()
                //param.job = $("#position1").val()
                param.job = $("#role1").val()
                param.remarks = $("#remark1").val()
                param.access = $("#position1").val()
                $.ajax({
                    type: "post",
                    url: Constants.path + "user/updateUser",
                    data: JSON.stringify(param),
                    dataType: "json",
                    contentType: "application/json",
                    success: function (data) {
                        $("#name").val("");
                        $("#superName").val("");
                        alert("修改成功！")
                        $('#myModal1').modal('hide');
                        getDataList(1, null)
                    }
                })
            })
            var getDataList = function (currPage, jg) {
                var param = { pageSize: pageSize, page: currPage }
                $.ajax({
                    url: Constants.path + "user/getUserList",
                    type: "post",
                    dataType: 'json',
                    data: JSON.stringify(param),
                    contentType: "application/json",
                    success: function (response) {
                        if (response) {
                            $("#Pagination").pagination(
                                {
                                    pageCount: Math.ceil(response.total / pageSize),
                                    totalData: response.total,
                                    showData: pageSize,
                                    callback: function (api) {
                                        getDataList(api.getCurrent(), null)
                                    }//回调函数
                                });
                            initFlag = false;
                            page = currPage
                            ractive.set("data", response);
                            Array.from(document.getElementsByTagName('input')).forEach(function (obj) {
                                obj.checked = false
                            })
                            if (sessionStorage.getItem('pageInfoArr')) {
                                var pageInfoArr = JSON.parse(sessionStorage.getItem('pageInfoArr'))
                                pageInfoArr.forEach(function (currentPageInfo) {
                                    if (currentPageInfo.page == currPage) {
                                        if (currentPageInfo.checked.length > 0) {
                                            currentPageInfo.checked.forEach(function (checkBox) {
                                                if (checkBox) {
                                                    document.getElementById(checkBox).checked = true;
                                                }
                                            })
                                        }
                                    }

                                })
                            }
                        } else {
                            //暂无数据
                        }
                    }
                });
            }
            return {
                init: function () {
                    renderView();
                }
            }
        }();
        $(document).ready(address_app.init)
    </script>
</body>

</html>