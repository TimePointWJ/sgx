    <script>
        $(function () {
        	var user = store.get("user");
        	console.log(user);
        	if(typeof(user) == 'undefined'){
        		window.parent.location.href = "login.html";
        	}
        	$("input[name='higherUnion']").val(user.higherUnion);
        	$("input[name='buildTeam']").val(user.union);
        	$("input[name='comName']").val(user.union);
            var build={};
            var team=[];
            var candidate={};
            var candidate1={};
            var unioninfo=[];
            var unionpayinfo=[];
            $("input[name='comName']").keyup(function () {
                $("input[name='comName']").val($(this).val());
            })
            $("input[name='higherUnion']").keyup(function () {
                $("input[name='higherUnion']").val($(this).val());
            })
            $("input[name='teamMember']").keyup(function () {
                $("input[name='teamMember']").val($(this).val());
            })
            $("input[name='buildTeamNumber']").keyup(function () {
                $("input[name='buildTeamNumber']").val($(this).val());
            })
            $("input[name='teamLeader']").keyup(function () {
                $("input[name='teamLeader']").val($(this).val());
            })
            $("input[name='deputyLeader']").keyup(function () {
                $("input[name='deputyLeader']").val($(this).val());
            })
            $("input[name='applyYear']").keyup(function () {
                $("input[name='applyYear']").val($(this).val());
            })
            $("input[name='applyMouth']").keyup(function () {
                $("input[name='applyMouth']").val($(this).val());
            })
            $("input[name='applyDay']").keyup(function () {
                $("input[name='applyDay']").val($(this).val());
            })

            //填表时间
                var time='';

            $("#niban").click(function () {
                //填表时间
                $("input[name='fillInTime']").each(function () {
                    time+=$(this).val()+'-';
                })
                time=time.substr(0,time.length-1);
                //build
                build={
                    "buildYear": $("input[name='buildYear']").val(),
                    "buildMouth": $("input[name='buildMouth']").val(),
                    "buildDay": $("input[name='buildDay']").val(),
                    "approver": $("input[name='approver']").val(),
                    "workers": $("input[name='workers']").val(),
                    "womenWorkers": $("input[name='womenWorkers']").val(),
                    "buildTeam": $("input[name='teamMember']").val(),
                    "buildTeamNumber": $("input[name='buildTeamNumber']").val(),
                    "teamLeader": $("input[name='teamLeader']").val(),
                    "deputyLeader": $("input[name='deputyLeader']").val(),
                    "title": "关于成立"+$("input[name='comName']").val()+"工会组织的请示",
                    "comName": $("input[name='comName']").val(),
                    "applyYear": $("input[name='applyYear']").val(),
                    "applyMouth": $("input[name='applyMouth']").val(),
                    "applyDay": $("input[name='applyDay']").val(),
                    "processingState": "01",//新建默认01
                    "higherUnion": $("input[name='higherUnion']").val(),
                    "higherUnionId": user.higherUnionId,
                    "contacts": $("input[name='contacts']").val(),
                    "contactsPhone": $("input[name='contactsPhone']").val(),
                    "fillInTime": time,
                };
                //team
                $("#table1 tbody tr").each(function () {
                    var name=$(this).find("input[name='name']").val();
                    var sex=$(this).find("input[name='sex']").val();
                    var birthday=$(this).find("input[name='birthday']").val();
                    var educationDegree=$(this).find("input[name='educationDegree']").val();
                    var politicalOutlook=$(this).find("input[name='politicalOutlook']").val();
                    var post=$(this).find("input[name='post']").val();
                    var contactInfo=$(this).find("input[name='contactInfo']").val();
                    var array={
                        "name": name,
                        "sex": sex,
                        "birthday": birthday,
                        "educationDegree": educationDegree,
                        "politicalOutlook": politicalOutlook,
                        "post": post,
                        "unionPost": "",
                        "contactInfo": contactInfo
                    }
                    if(array.name){
                    	team.push(array);
                    }
                })

                //candidate
                //candidate1
                var comName=$("#table2 tbody tr input[name='comName']").val();
                var registrationNumber=$("#table2 tbody tr input[name='registrationNumber']").val();
                var comAddress=$("#table2 tbody tr input[name='comAddress']").val();
                var legalPerson=$("#table2 tbody tr input[name='legalPerson']").val();
                var unionName=$("#table2 tbody tr input[name='unionName']").val();
                var contactInfo=$("#table2 tbody tr input[name='contactInfo']").val();
                var emp=$("#table2 tbody tr input[name='emp']").val();
                var womenEmp=$("#table2 tbody tr input[name='womenEmp']").val();
                var unionNum=$("#table2 tbody tr input[name='unionNum']").val();
                var womenUnionNum=$("#table2 tbody tr input[name='womenUnionNum']").val();
                candidate1={
                    "comName": comName,
                    "registrationNumber": registrationNumber,
                    "comAddress": comAddress,
                    "legalPerson": legalPerson,
                    "unionName": unionName,
                    "contactInfo": contactInfo,
                    "emp": emp,
                    "womenEmp": womenEmp,
                    "unionNum": unionNum,
                    "womenUnionNum": womenUnionNum
                }
                //unioninfo
                $("#table2 tbody .sub1").each(function () {
                    var name1=$(this).find("input[name='name']").val();
                    var sex1=$(this).find("input[name='sex']").val();
                    var birthday1=$(this).find("input[name='birthday']").val();
                    var educationDegree1=$(this).find("input[name='educationDegree']").val();
                    var politicalStatus1=$(this).find("input[name='politicalStatus']").val();
                    var joinTime1=$(this).find("input[name='joinTime']").val();
                    var unionPosition1=$(this).find("input[name='unionPosition']").val();
                    var comPosition1=$(this).find("input[name='comPosition']").val();
                    var note1=$(this).find("input[name='note']").val();
                    var array1={
                        "name": name1,
                        "sex": sex1,
                        "birthday": birthday1,
                        "educationDegree": educationDegree1,
                        "politicalStatus": politicalStatus1,
                        "joinTime": joinTime1,
                        "unionPosition": unionPosition1,
                        "comPosition": comPosition1,
                        "note": note1
                    }
                    if(array1.name){
                    	unioninfo.push(array1);
                    }
                })
                //unionpayinfo
                $("#table2 tbody .sub2").each(function () {
                    var name2=$(this).find("input[name='name']").val();
                    var sex2=$(this).find("input[name='sex']").val();
                    var birthday2=$(this).find("input[name='birthday']").val();
                    var educationDegree2=$(this).find("input[name='educationDegree']").val();
                    var politicalStatus2=$(this).find("input[name='politicalStatus']").val();
                    var joinTime2=$(this).find("input[name='joinTime']").val();
                    var unionPosition2=$(this).find("input[name='unionPosition']").val();
                    var comPosition2=$(this).find("input[name='comPosition']").val();
                    var note2=$(this).find("input[name='note']").val();
                    var array3={
                        "name": name2,
                        "sex": sex2,
                        "birthday": birthday2,
                        "educationDegree": educationDegree2,
                        "politicalStatus": politicalStatus2,
                        "joinTime": joinTime2,
                        "unionPosition": unionPosition2,
                        "comPosition": comPosition2,
                        "note": note2
                    }
                    unionpayinfo.push(array3);
                })

                $.ajax({
                    type:"post",
                    url: Constants.path+"union/saveUnion",
                    dataType : 'json',
                    contentType : "application/json",
                    data:JSON.stringify({
                        "build": build,
                        "team": team,
                        "candidate": {//工会委员会和经审委员会委员候选人情况表
                            "candidate": candidate1,
                            "unioninfo": unioninfo,
                            "unionpayinfo": unionpayinfo
                        }
                    }),
                    success: function (data) {
                         //window.location="MainMenuElection.html";

                    }
                })
            })

        })
    </script>
