.map_party_card {
    font-size: 20px;
    background-image: url('../../i/dangyuanbg.png');
    background-size: contain;
    background-repeat: no-repeat;
}

    .map_party_card a {
        color: #e91313;
    }

.map_top_nav {
    position: fixed;
    top: 80px;
    left: 90px;
    z-index: 1002;
}

    .map_top_nav div {
        display: inline-block;
        width: 64px;
        text-align: center;
        margin: 0 8px;
    }

        .map_top_nav div span {
            color: #ffffff;
        }

    .map_top_nav a {
    }

        .map_top_nav a img:hover {
            animation: markerSwing 0.93s infinite;
            -webkit-animation: markerSwing .93s infinite ease;
        }

@keyframes markerSwing {
    0% {
        -webkit-transform: scale(1);
    }

    15% {
        -webkit-transform: scale(1.2);
    }

    30% {
        -webkit-transform: scale(1);
    }

    55% {
        -webkit-transform: scale(1.1);
    }

    100% {
        -webkit-transform: scale(1);
    }
}

@-webkit-keyframes markerSwing {
    0% {
        -webkit-transform: scale(1);
    }

    15% {
        -webkit-transform: scale(1.2);
    }

    30% {
        -webkit-transform: scale(1);
    }

    55% {
        -webkit-transform: scale(1.1);
    }

    100% {
        -webkit-transform: scale(1);
    }
}

